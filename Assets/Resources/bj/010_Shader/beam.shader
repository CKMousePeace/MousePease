				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 texCoord83 = IN.ase_texcoord3.xy * float2( 0.34,1 ) + float2( 0.332,0 );
				float2 temp_cast_0 = (texCoord83.x).xx;
				float2 texCoord76 = IN.ase_texcoord3.xy * float2( 1,0.34 ) + float2( 0,0.332 );
				float2 texCoord48 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0.37 );
				float2 panner45 = ( 0.5 * _Time.y * float2( -10,5 ) + texCoord48);
				float4 temp_output_55_0 = ( ( tex2D( _TextureSample1, temp_cast_0 ) * tex2D( _TextureSample0, texCoord76 ) ) * tex2D( _ggg, panner45 ) );
				
				float2 texCoord67 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0.37 );
				float2 panner66 = ( 0.13 * _Time.y * float2( -25,-2 ) + texCoord67);
				float smoothstepResult80 = smoothstep( 0.2 , -0.63 , tex2D( _TextureSample2, panner66 ).r);
				float2 temp_cast_2 = (texCoord83.x).xx;
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( ( IN.ase_color * temp_output_55_0 ) * _inten ).rgb;
				float Alpha = ( (0.0 + (smoothstepResult80 - 0.0) * (_beamalpha - 0.0) / (1.0 - 0.0)) * temp_output_55_0 * IN.ase_color.a ).r;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
		}

		
